  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "doc": "node_modules/.bin/yuidoc",
    "lint": "node_modules/.bin/eslint src spec"
  },
  "jest": {
    "automock": false,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/tests/fixtures/",
      "/tests/support/",
      "/dist/",
      "/node_modules/",
      "/rollup.config.js",
      "/setupJest.js"
    ]
  },
  "files": [
    "dist"
  ]
