{
  "tags": {
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "src/FactoryFarm.js",
      "src/MockServer.js",
      "src/Model.js",
      "src/relationships.js",
      "src/Store.js",
      "src/testUtils.js",
      "src/utils.js"
    ]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "readme": "README.md"
  }
}
